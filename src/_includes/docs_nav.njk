<nav class="lg:text-sm lg:leading-6 relative mt-5 lg:mt-10">
    <div class='lg:hidden flex justify-end'>
        <button id="nav-menu-close" class="border p-1 rounded-md shadow">
            <svg xmlns="http://www.w3.org/2000/svg"
                 width="24"
                 height="24"
                 viewBox="0 0 24 24"
                 fill="none"
                 stroke="currentColor"
                 stroke-width="2"
                 stroke-linecap="round"
                 stroke-linejoin="round"
                 class="h-4 w-4">
                <line x1="18" x2="6" y1="6" y2="18"></line><line x1="6" x2="18" y1="6" y2="18"></line>
            </svg>
        </button>
    </div>
    <ul>
        {% for link in data.links %}
            <li>
                <a class="group flex items-center lg:text-sm lg:leading-6 mb-4 font-medium {% if link.url | startsWith(data.baseUrl) -%}text-sky-500 dark:text-sky-400{% else %}text-slate-700 hover:text-slate-900 dark:text-slate-400 {% endif %}"
                   href="{{ link.url }}"
                   {% if link.url | startsWith('http') %}target="_blank"{% endif %}>
                    <div class="mr-4 rounded-md ring-1 ring-slate-900/5 shadow-sm group-hover:shadow group-hover:ring-slate-900/10 dark:ring-0 dark:shadow-none dark:group-hover:shadow-none dark:group-hover:highlight-white/10 group-hover:shadow-{{ link.outline }}-200 dark:group-hover:bg-sky-500 dark:bg-sky-500 dark:highlight-white/10 p-1">
                        {{ link.icon | safe }}
                    </div>
                {{ link.title }}</a>
            </li>
        {% endfor %}
        {% for section in data.sections %}
            <li class="mt-12 lg:mt-8">
                <h5 class="mb-8 lg:mb-3 font-semibold text-slate-900 dark:text-slate-200">{{ section.title }}</h5>
                <ul class="space-y-6 lg:space-y-2 border-l border-slate-100 dark:border-slate-800">
                    {% for link in section.pages %}
                        <li data-active="true">
                            <a class="block border-l pl-4 -ml-px {% if page.url == link.url -%}text-sky-500 border-current font-semibold dark:text-sky-400{% else %}border-transparent hover:border-slate-400 dark:hover:border-slate-500 text-slate-700 hover:text-slate-900 dark:text-slate-400 dark:hover:text-slate-300{%- endif %} "
                               href="{{ link.url }}"
                               {% if link.url | startsWith('http') %}target="_blank"{% endif %}>{{ link.title }}</a>
                        </li>
                    {% endfor %}
                </ul>
            </li>
        {% endfor %}
    </ul>
</nav>
