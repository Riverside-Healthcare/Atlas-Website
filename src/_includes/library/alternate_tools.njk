{% for t in tools %}
    <div class="{% if loop.index == 1 %}rounded-lg outline -outline-offset-4 outline-cyan-500 outline-2 border z-10{% endif %}">
        <div class="font-medium text-lg h-12 flex px-4 border-b bg-slate-100">
            <span class="my-auto text-center w-max">{{ t.name }}</span>
        </div>
        <div class="text-center">
            {% for f in t.features %}
                <div class="flex h-10 odd:bg-white even:bg-slate-50 px-5 group"
                     data-feature="{{ f.name }}">
                    <span class="my-auto mx-auto">
                        {% if f.value == "y" %}
                            <svg xmlns="http://www.w3.org/2000/svg"
                                 width="24"
                                 height="24"
                                 viewBox="0 0 24 24"
                                 fill="none"
                                 stroke="currentColor"
                                 stroke-width="2"
                                 stroke-linecap="round"
                                 stroke-linejoin="round"
                                 class="h-5 w-5 text-emerald-500 group-hover:text-emerald-600">
                                <polyline points="9 11 12 14 22 4"></polyline>
                                <path d="M21 12v7a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11"></path>
                            </svg>
                        {% elif f.value == "n" %}
                            <svg xmlns="http://www.w3.org/2000/svg"
                                 width="24"
                                 height="24"
                                 viewBox="0 0 24 24"
                                 fill="none"
                                 stroke="currentColor"
                                 stroke-width="2"
                                 stroke-linecap="round"
                                 stroke-linejoin="round"
                                 class="h-5 w-5 text-pink-500 group-hover:pink-emerald-600">
                                <rect width="18" height="18" x="3" y="3" rx="2" ry="2"></rect><line x1="9" x2="15" y1="9" y2="15"></line><line x1="15" x2="9" y1="9" y2="15"></line>
                            </svg>
                        {% else %}
                            <span>-</span>
                        {% endif %}
                    </span>
                </div>
            {% endfor %}
            <div class="h-12 bg-slate-100 border-y" data-group="Integrations">&nbsp;</div>
            {% for i in t.integrations %}
                <div class="flex h-10 odd:bg-white even:bg-slate-50 px-5 group"
                     data-feature="{{ i.name }}">
                    <span class="my-auto mx-auto">
                        {% if i.value == "y" %}
                            <svg xmlns="http://www.w3.org/2000/svg"
                                 width="24"
                                 height="24"
                                 viewBox="0 0 24 24"
                                 fill="none"
                                 stroke="currentColor"
                                 stroke-width="2"
                                 stroke-linecap="round"
                                 stroke-linejoin="round"
                                 class="h-5 w-5 text-emerald-500 group-hover:text-emerald-600">
                                <polyline points="9 11 12 14 22 4"></polyline>
                                <path d="M21 12v7a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h11"></path>
                            </svg>
                        {% elif i.value == "n" %}
                            <svg xmlns="http://www.w3.org/2000/svg"
                                 width="24"
                                 height="24"
                                 viewBox="0 0 24 24"
                                 fill="none"
                                 stroke="currentColor"
                                 stroke-width="2"
                                 stroke-linecap="round"
                                 stroke-linejoin="round"
                                 class="h-5 w-5 text-pink-500 group-hover:pink-emerald-600">
                                <rect width="18" height="18" x="3" y="3" rx="2" ry="2"></rect><line x1="9" x2="15" y1="9" y2="15"></line><line x1="15" x2="9" y1="9" y2="15"></line>
                            </svg>
                        {% else %}
                            <span>-</span>
                        {% endif %}
                    </span>
                </div>
            {% endfor %}
        </div>
        {% if t.url %}
            <div class="h-14 flex flex-row px-2 mx-2">
                <a class="py-2 px-4 m-auto w-max rounded-md border hover:outline hover:outline-2 hover:outline-blue-500/50"
                   href="{{ t.url }}"
                   {% if t.url | startsWith("http") %}target="_blank"{% endif %}
                   rel="noopener">Open Docs</a>
            </div>
        {% endif %}
    </div>
{% endfor %}
</div>
