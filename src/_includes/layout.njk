    <body>
        {% include "src/_includes/nav.njk" %}
        <section class="is-flex-grow-1">
            {% block body %}{% endblock body %}
        </section>
        {% include "src/_includes/foot.njk" %}
        <script type="text/javascript" src="{{ ASSET_URL }}"></script>
