        <meta property="og:url" content="https://atlas.bi" />
        <meta property="og:type" content="website" />
        <meta property="og:title" content="{{ title or 'Atlas of Information Management' }}" />
        <meta property="og:image" content="https://atlas.bi/static/img/logo-192x192.png"/>
